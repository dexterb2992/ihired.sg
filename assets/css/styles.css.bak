	}
	.bam > img {
		position:absolute;
		top:1px;
		left:0px;
	}
	.bam > .joinus {
		position:absolute;
		top:20px;
		left:410px;

	}
	
	.bam > .career {
		position:absolute;
		top:-28px;
		left:740px;
	}
	
	
	.bam > div > input[type="text"] {
		width:100%;
		border:3px solid #e1e1e1;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
		padding:8px 15px;
		margin-bottom:20px;
		font-family: 'Lato', Arial, sans-serif;
		font-size:13px;
		font-size:1.3rem;
