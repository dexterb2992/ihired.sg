	a:active { color:#868686; text-decoration:none;}
	a:hover { color:#dd2526; text-decoration:none;}
	
	/* Buttons */
	a.btn-red {
		font-family: 'Klavika', Arial, sans-serif;
		font-size:15px;
		font-size:1.5 rem;
		font-weight:900;
		text-transform:uppercase;
		color:#ffffff;
		background-color:#dd2526;
		height:35px;
		line-height:35px;
		display:block;
		padding:0 15px;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
	}
	a.btn-red:hover {
		color:#dd2526;
		background-color:#ffffff;
	}
	input.btn-red {
		font-family: 'Klavika', Arial, sans-serif;
		font-size:15px;
		font-size:1.5 rem;
		font-weight:900;
		text-transform:uppercase;
		color:#ffffff;
		background-color:#dd2526;
		height:35px;
		line-height:35px;
		display:block;
		padding:0 15px;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
		border:0px solid #ffffff;
	}
	
