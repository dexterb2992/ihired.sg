	nav .widget {
		width:250px;
		border:1px solid #e5e5e5;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
		position:absolute;
		top:10px;
		right:0;
		font-family: 'Klavika', Arial, sans-serif;
		text-transform:uppercase;
		color:#868686;
	}
	.widget > div.clearfix > div:nth-child(2) {
		float:left;
		width:125px;
		text-align:center;
	}
	.widget > div.clearfix > div:first-child {
		border-right:1px solid #e5e5e5;
		font-weight:700;
		width:124px;
		float:left;
		text-align:center;
	}
	.widget > div.clearfix > div:first-child, .widget > div.clearfix > div:nth-child(2) {
		border-bottom:1px solid #e5e5e5;
		padding:5px 0;
	}
	.widget > div.clearfix > div:first-child > img {
		margin-right:5px;
		position:relative;
		top:-1px;
	}
	.widget > div.fb-like{
		text-align:center;
		padding:5px 15px;
	}
	
	
	.locatiob-div {
		margin-bottom:20px;
		float:left;
	}
	
    .line {
		margin-top:-18px;
	}
	
	
	.content-inside > h1 {
		font-size:2em;
		color:#de2626;
		text-transform:uppercase;
		font-weight:900;
