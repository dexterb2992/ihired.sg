	.middlelogos {
		width:1023px;
		margin:auto;
		text-align:center;
	}
	
	.sol-wrap {
		width:1023px;
		margin:0 auto;
		padding-left:2px;
	}
	.sol-wrap > div {
		width:19%;
		float:left;
		outline:1px solid #dddddd;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
		position:relative;
		height:340px;
		margin-bottom:60px;
		margin-right:12px;
	}
	
	.sol-wrap > div > img {
		width:100%;
	}
	
	.sol-wrap > div:last-child {
		margin-right:0;
	}
	.sol-wrap h2 {
		margin:20px;
		padding-left:30px;
		background-image:url('../img/icons.png');
		background-repeat:no-repeat;
		background-position:1px 1px;
	}
	.sol-wrap h2.tr { background-position:1px -42px; }
	.sol-wrap h2.hr { background-position:1px -86px; }
	.sol-wrap h2.or { background-position:1px -131px; }
	.sol-wrap h2.pd { background-position:1px -174px; }
	
	.sol-wrap > div > ul {
		margin:0;
		padding:0;
		list-style:none;
		color:#8b8a8a;
		padding:20px;
	}
	
	.sol-wrap > div > a {
		position:absolute;
		bottom:20px;
		right:20px;
		color:#e13c3d;
	}
	
	.bam {
		width:1023px;
		margin:0 auto;
		-webkit-border-radius: 10px;
		-moz-border-radius: 10px;
		border-radius: 10px;
		background: #ffffff;
		background: -moz-linear-gradient(top,  #ffffff 1%, #f0f0f0 98%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#ffffff), color-stop(98%,#f0f0f0));
